<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from VoucherDetailService WSDL service -->

  <service name="VoucherDetailPort" provider="java:RPC" style="document" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://dev.com/voucherdetail"/>
      <parameter name="wsdlServiceElement" value="VoucherDetailService"/>
      <parameter name="schemaQualified" value="http://dev.com/voucherdetail"/>
      <parameter name="wsdlServicePort" value="VoucherDetailPort"/>
      <parameter name="className" value="com.dev.voucherdetail.VoucherDetailBindingSkeleton"/>
      <parameter name="wsdlPortType" value="VoucherDetailPortType"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>

      <typeMapping
        xmlns:ns="http://dev.com/voucherdetail"
        qname="ns:GetVoucherDetail_INTO"
        type="java:com.dev.voucherdetail.GetVoucherDetail_INTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dev.com/voucherdetail"
        qname="ns:VoucherDetail_TO"
        type="java:com.dev.voucherdetail.VoucherDetail_TO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dev.com/voucherdetail"
        qname="ns:VoucherDetailFault"
        type="java:com.dev.voucherdetail.VoucherDetailFault"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
